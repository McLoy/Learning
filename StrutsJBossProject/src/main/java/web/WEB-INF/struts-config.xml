<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>

    <form-beans>
        <form-bean name="mainForm" type="ua.vtkachenko.form.MainForm" />
    </form-beans>

    <action-mappings>
        <action path="/mainForm" type="ua.vtkachenko.action.MainAction" name="mainForm">
            <forward name="success" path="/mainForm.jsp"/>
        </action>
    </action-mappings>
</struts-config>